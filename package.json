{
  "name": "@saucelabs/cypress-plugin",
  "version": "0.2.0",
  "description": "Sauce Cypress Plugin to report your results directly to Sauce Labs",
  "main": "src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint",
    "release": "release-it --github.release",
    "release:ci": "npm run release -- --ci --no-git.requireCleanWorkingDir",
    "release:patch": "npm run release -- patch",
    "release:minor": "npm run release -- minor",
    "release:major": "npm run release -- major"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/saucelabs/sauce-cypress-plugin.git"
  },
  "bugs": {
    "url": "https://github.com/saucelabs/sauce-cypress-plugin/issues"
  },
  "author": "devx <devx@saucelabs.com>",
  "files": [
    "src/**/*"
  ],
  "license": "MIT",
  "dependencies": {
    "@saucelabs/sauce-json-reporter": "^0.0.2",
    "chalk": "^4.1.2",
    "cli-table3": "^0.6.0",
    "saucelabs": "^7.0.4"
  },
  "devDependencies": {
    "axios": "^0.22.0",
    "cypress": "^8.5.0",
    "eslint": "^7.32.0",
    "jest": "^27.2.4",
    "release-it": "^14.11.6"
  }
}
